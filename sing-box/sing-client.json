        "rules":
        [
            {
                "domain":
                [
                    "ghproxy.com",
                    "cdn.jsdelivr.net",
                    "testingcf.jsdelivr.net"
                ],
                "server": "localDns"
            },
            {
                "rule_set": "geosite-category-ads-all",
                "server": "block"
            },
            {
                "outbound": "any",
                "server": "localDns",
                "disable_cache": true
            },
            {
                "rule_set":
                [
                    "geosite-cn"
                ],
                "server": "localDns"
            },
            {
                "clash_mode": "direct",
                "server": "localDns"
            },
            {
                "clash_mode": "global",
                "server": "proxyDns"
            },
            {
                "rule_set": "geosite-geolocation-!cn",
                "server": "proxyDns"
            },
            {
                "rule_set":
                [
                    "geosite-category-ads-all"
                ],
                "server": "block"
            },
            {
                "query_type":
                [
                    "A",
                    "AAAA"
                ],
                "server": "remote"
            }
        ],
        "fakeip":
        {
            "enabled": true,
            "inet4_range": "198.18.0.0/15",
            "inet6_range": "fc00::/18"
        },
        "final": "proxyDns",
        "independent_cache": true,
        "strategy": "ipv4_only",
        "disable_cache": false,
        "disable_expire": false
    },
